  <body class="text-center">
    <form action ="/logInUser" method ='POST' >
      <h1 class="h3 mb-3 font-weight-normal">Please Log In</h1>
      <a href="/" class="btn btn-secondary my-2">Home</a>

      <label for="inputUser" class="sr-only">Username</label>
      <input id="username" name="name" class="form-control" placeholder="Username" required autofocus>

      <label for="inputPassword" class="sr-only">Password</label>
      <input id="inputPassword" name="pass" class="form-control" placeholder="Password" required>

      <button type="submit" class="btn btn-lg btn-primary btn-block">Log In!</button>

    </form>
  </body>
